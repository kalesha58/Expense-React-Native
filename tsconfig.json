{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "typeRoots": ["node_modules/@types","src/types"],
    "types": ["jest"],
    "baseUrl": "./src/",
    "paths": {
      "@assets/*":["assets/*"],
      "@components/*":["components/*"],
      "@constants/*":["constants/*"],
      "@context/*":["context/*"],
      "@hooks/*":["hooks/*"],
      "@navigation/*":["navigation/*"],
      "@screens/*":["screens/*"],
      "@service/*":["service/*"],
      "@types/*":["types/*"],
      "@utils/*":["utils/*"],
      "@api/*":["api/*"],
      "@services/*":["services/*"],
      "@styles/*":["styles/*"],
      "@config/*":["config/*"]
    }
  }
}
